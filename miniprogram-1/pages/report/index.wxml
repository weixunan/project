          </view>
          <view class="flex-row items-center equal-division-item_2">
            <image
              class="image_3"
              src="../assets/table.png"
            />
            <text class="ml-8 font_2 text_6">入库统计</text>
          </view>
        </view>
        <text class="mt-18 self-start font_2">出库</text>
